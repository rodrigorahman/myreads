{
  "name": "myreads",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-progress-2": "^4.4.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-test-renderer": "^16.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --setupFiles='./src/__setups__/setupTest.js' --setupTestFrameworkScriptFile=raf/polyfill.js",
    "test:coverage": "react-scripts test --env=jsdom --setupFiles='./src/__setups__/setupTest.js' --setupTestFrameworkScriptFile=raf/polyfill.js --coverage",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,sx}",
      "!src/domain/*",
      "!src/index.js",
      "!src/BooksAPI.js",
      "!src/App.js",
      "!src/registerServiceWorker.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "core-js": "^2.5.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "jest-cli": "^21.2.1",
    "raf": "^3.3.2"
  }
}
